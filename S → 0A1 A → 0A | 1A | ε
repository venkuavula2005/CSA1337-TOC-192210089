#include <stdio.h>

int main() {
    char s[101];
    scanf("%s", s);
    char *ptr = s;
    while (*ptr == '0') ptr++;
    while (*ptr == '1') ptr++;
    printf("%s\n", *ptr == '\0' ? "String belongs to the language." : "String does not belong to the language.");
    return 0;
}
input 00110
string belongs to the language
